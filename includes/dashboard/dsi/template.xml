	<content lang='fr_FR'><![CDATA[
		<h2>Bannettes</h2>
		<div class='row'>
			<div class='title'>Total : </div>
			<div class='value'>{{nb_bannettes.0.total}}</div>
		</div>
		<div class='row'>
			<div class='title'>dont à diffuser : </div>
			<div class='value'>{{nb_bannettes_diff.0.total}}</div>
		</div>
